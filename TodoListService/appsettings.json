{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Tenant": "https://contoso.onmicrosoft.com", // for instance contoso.onmicrosoft.com
    "TenantId": "xxxxxxx-xxxx......", //Tenant Id of the Azure AD Tenant
    "ClientId": "xxxxxxx-xxxx......", // Application Id of App Registered in Azure Ad App Registrations V2
    "RequireHttpsMetadata": false
  },
  "AzureAdB2C": {
    "Instance": "https://login.microsoftonline.com/",
    "Tenant": "contosob2c.onmicrosoft.com",
    "ClientId": "xxxxx-xxxx-.........", //Application Id of App Registered in Azure Ad B2c
    "Policy": "B2C_1_SiUpIn", // Signin Signout policies created in B2C
    "RequireHttpsMetadata": false,
    "ScopeRead": "https://contosob2c.onmicrosoft.com/api/demo.read",
    "ScopeWrite": "https://contosob2c.onmicrosoft.com/api/demo.read"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
